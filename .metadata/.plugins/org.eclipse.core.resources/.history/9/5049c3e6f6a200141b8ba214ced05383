package gui;

import java.awt.Dimension;

import javax.swing.JFrame;
import javax.swing.JPanel;


public class GUI extends JFrame {
	
	private JPanel panel;
		public GUI()
		{
			setSize(new Dimension(1000,1000));
			setTitle("DB Projekt");
			setDefaultCloseOperation(EXIT_ON_CLOSE);
			
			panel = new JPanel();
			
			setVisible(true);
		}
}
